#include <16F877A.h>
//#include <def_877a.h>
//#device *=16 adc=10
#fuses XT, NOWDT, NOPROTECT, BROWNOUT, PUT, NOLVP 
#use delay(clock=20000000)

void main(){
   setup_timer_2(T2_DIV_BY_16,5000,1);
   setup_ccp2(CCP_PWM);
   setup_ccp1(CCP_PWM);
   output_low(PIN_C1); 
   output_high(PIN_C2); 
   set_pwm2_duty(250);
   set_pwm1_duty(250);
   
   while(true){
    }  
}

